<scenario name="Basic Sipstone UAC">
  <!-- In client mode (sipp placing calls), the Call-ID MUST be         -->
  <!-- generated by sipp. To do so, use [call_id] keyword.                -->
  <send retrans="500">
    <![CDATA[

      INVITE sip:[service]@[remote_ip]:[remote_port] SIP/2.0
      Via: SIP/2.0/[transport] [local_ip]:[local_port];branch=[branch]
      From: sipp <sip:9193377202@[local_ip]:[local_port]>;isup-oli=62;tag=[call_number]
      To: sut <sip:[service]@[remote_ip]:[remote_port]>
      Call-ID: [call_id]
      CSeq: 1 INVITE
      Contact: sip:9193377202@[local_ip]:[local_port]
      Max-Forwards: 70
      Subject: Performance Test
      Min-SE: 3600
      Content-Type: application/sdp
      Content-Length: [len]

      v=0
      o=user1 53655765 2353687637 IN IP[local_ip_type] [local_ip]
      s=-
      c=IN IP[media_ip_type] [media_ip]
      t=0 0
      m=audio [media_port] RTP/AVP 0 101
      a=rtpmap:101 telephone-event/8000
      a=fmtp:101 0-15
      a=ptime:30
      a=sendrecv

    ]]>
  </send>

  <recv response="100"
        optional="true">
  </recv>

    <recv response="180" >
    <action>
      <ereg regexp=".*" search_in="hdr" header="From:" check_it="true" 
assign_to="1" />
      <ereg regexp=".*" search_in="hdr" header="To:" check_it="true" 
assign_to="2" />
      <ereg regexp=".*" search_in="hdr" header="Via:" check_it="true" 
assign_to="3" />
      <ereg regexp=".*" search_in="hdr" header="Call-ID:" check_it="true" 
assign_to="4" />
      <log message="Received 180 response, the From header is: [$1]"/>
      <log message="Received 180 response, the To header is: [$2]"/>
      <log message="Received 180 response, the Via header is: [$3]"/>
      <log message="Received 180 response, the CallID header is: [$4]"/>
    </action>
  </recv>
 


 <send>
    <![CDATA[
	CANCEL sip:[service]@[remote_ip]:[remote_port] SIP/2.0
	From: [$1]
	To: [$2]
	Via: [$3]
	Max-Forwards: 70
	Call-ID: [$4]
	CSeq: 1 CANCEL
	Contact: sip:9193377202@[local_ip]:[local_port]
    ]]>
 </send>
 
 <recv response="200" optional="true">
 </recv>

  <recv response="487" >
  </recv>
 

  <!-- By adding rrs="true" (Record Route Sets), the route sets         -->
  <!-- are saved and used for following messages sent. Useful to test   -->
  <!-- against stateful SIP proxies/B2BUAs.                             -->

 <send>
    <![CDATA[

      ACK sip:[service]@[remote_ip]:[remote_port] SIP/2.0
      Via: SIP/2.0/[transport] [local_ip]:[local_port];branch=[branch]
      [routes]
      From: sipp <sip:9193377202@[local_ip]:[local_port]>;tag=[call_number]
      To: sut <sip:[service]@[remote_ip]:[remote_port]>[peer_tag_param]
      Call-ID: [call_id]
      CSeq: 1 ACK
      Contact: sip:9193377202@[local_ip]:[local_port]
      Max-Forwards: 70
      Subject: Performance Test
      Content-Length: 0

    ]]>
  </send>



  <!-- This delay can be customized by the -d command-line option       -->
